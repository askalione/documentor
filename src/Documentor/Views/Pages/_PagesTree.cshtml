@model IEnumerable<NavItem>

@if (Model.Count() > 0)
{
    <ul>
        @foreach (NavItem navItem in Model)
        {
            <li class="jstree-open" data-virtual-path="@navItem.VirtualPath">
                @navItem.DisplayName
                @await Html.PartialAsync("_PagesTree", navItem.Children)
            </li>
        }
    </ul>    
}